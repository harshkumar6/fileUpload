<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>fileUpload</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>insertDataServlet</servlet-name>
    <servlet-class>com.clink.Centurylink90DaysData</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>insertDataServlet</servlet-name>
    <url-pattern>/insertData</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>controlServlet</servlet-name>
    <servlet-class>FirstServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>controlServlet</servlet-name>
    <url-pattern>/myFirstServlet</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>sendEmailServlet</servlet-name>
    <servlet-class>com.clink.SendMailByDate</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>sendEmailServlet</servlet-name>
    <url-pattern>/sendEmail</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>uploadDFRServlet</servlet-name>
    <servlet-class>com.clink.UploadFile</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>uploadDFRServlet</servlet-name>
    <url-pattern>/uploadDFR</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>uploadFEEDServlet</servlet-name>
    <servlet-class>com.clink.UploadFile</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>uploadFEEDServlet</servlet-name>
    <url-pattern>/uploadFEED</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>uploadFileServlet</servlet-name>
    <servlet-class>com.clink.UploadFile</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>uploadFileServlet</servlet-name>
    <url-pattern>/uploadFile</url-pattern>
  </servlet-mapping>
  
</web-app>